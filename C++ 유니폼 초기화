#include <iostream>


class A {
public:
A() { std::cout << "A 의 생성자 호출!" << std::endl; }
};
class B {
public:
B(A a) { std::cout << "B 의 생성자 호출!" << std::endl; }
};
int main() {
B b(A()); // 뭐가 출력될까요?
}
//============================================================
#include <iostream>


class A {
public:
A() { std::cout << "A 의 생성자 호출!" << std::endl; }
};


int main() {
 A a{};
}


//============================================================
#include <iostream>


class A {
public:
A(int x, double y) { std::cout << "A 의 생성자 호출!" << std::endl; } };

A func(){
  return {1,2.3};
}


int main() { func();}

